    "@types/node": "^17.0.32",
    "@types/react": "^17",
    "@types/react-redux": "^7.1.24",
    "aws-sdk": "^2.1132.0",
    "aws-sdk-mock": "^5.7.0",
    "i18next": "21.6.16",
