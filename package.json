{
  "name": "ReactNativeKatas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules",
    "build-ios-intel": "bundle install && bundle exec pod install --project-directory=ios",
    "build-ios-m1": "bundle install && arch -x86_64 bundle exec pod install --project-directory=ios",
    "start": "react-native start --reset-cache",
    "reset-cache": "yarn cache clean && timeout 5s react-native start --reset-cache",
    "test": "jest",
    "test-all": "jest --watchAll",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
